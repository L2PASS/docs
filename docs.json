{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "L2Pass Docs",
  "colors": {
    "primary": "#8C8DFC",
    "light": "#A5FFBE",
    "dark": "#26A17B"
  },
  "styling": {
    "codeblocks": {
      "theme": "dracula"
    }
  },
  "background": {
    "decoration": "grid",
    "color": {
      "light": "#FFFFFF",
      "dark": "#181818"
    }
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "icon": "book",
        "groups": [
          {
            "group": "Get Started",
            "pages": ["index"]
          },
          {
            "group": "Products",
            "pages": [
              {
                "group": "Yield",
                "pages": ["yiled"],
                "icon": "hand-holding-seedling",
                "expanded": true,
                "pages": [
                  {
                    "group": "Overnight Yield",
                    "tag": "9.75% APY",
                    "pages": [
                      "products/yiled/overview",
                      "products/yiled/architecture",
                      "products/yiled/user-flow",
                      "products/yiled/faq",
                      "products/yiled/risks"
                    ]
                  }
                ]
              },
              {
                "group": "Bridge",
                "icon": "bridge",
                "tag": "0% FEE",
                "expanded": true,
                "pages": [
                  "products/bridge/overview",
                  "products/bridge/faq"
                ]
              },
              {
                "group": "Gas Refuel",
                "icon": "gas-pump",
                "pages": [
                  "products/gas-refuel/",
                  "products/gas-refuel/contracts"
                ]
              },
              {
                "group": "NFT Quest",
                "icon": "map",
                "pages": [
                  "products/nft/",
                  "products/nft/contracts"
                ]
              }
            ]
          },
          {
            "group": "Resources",
            "pages": [
              "resources/components-test",
              "resources/brand-assets",
              "resources/contracts",
              "resources/roadmap",
              "resources/audits",
              "resources/support"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "L2PASS WEB ↗",
          "href": "https://l2pass.com/",
          "icon": "globe-pointer"
        },
        {
          "anchor": "YIELD ↗",
          "href": "https://earn.l2pass.com/",
          "icon": "hand-holding-seedling"
        },
        {
          "anchor": "BRIDGE ↗",
          "href": "https://swap.l2pass.com/",
          "icon": "bridge"
        },
             {
          "anchor": "GAS REFUEL ↗",
          "href": "https://l2pass.com/refuel",
          "icon": "gas-pump"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/l2pass_docs_light.svg",
    "dark": "/logo/l2pass_docs_dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "/resources/support"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Start Earning",
      "href": "https://earn.l2pass.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/l2_pass",
      "github": "https://github.com/l2pass",
      "telegram": "https://t.me/l2pass"
    }
