{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "L2Pass Docs",
  "colors": {
    "primary": "#8C8DFC",
    "light": "#A5FFBE",
    "dark": "#26A17B"
  },
  "styling": {
    "codeblocks": {
      "theme": "dracula"
    }
  },
  "background": {
    "decoration": "grid",
    "color": {
      "light": "#FFFFFF",
      "dark": "#181818"
    }
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "WEBSITE ↗",
          "icon": "globe-pointer",
          "href": "https://l2pass.com/"
        },
        {
          "anchor": "VAULT ↗",
          "icon": "hand-holding-seedling",
          "href": "https://vault.l2pass.com/"
        },
        {
          "anchor": "EARN ↗",
          "icon": "hand-holding-seedling",
          "href": "https://earn.l2pass.com/"
        },
        {
          "anchor": "SWAP ↗",
          "icon": "bridge",
          "href": "https://swap.l2pass.com/"
        },
        {
          "anchor": "GAS REFUEL ↗",
          "icon": "gas-pump",
          "href": "https://l2pass.com/refuel"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Guides",
        "icon": "book",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "index"
            ]
          },
          {
            "group": "Products",
            "pages": [
              {
                "group": "Yield",
                "icon": "hand-holding-seedling",
                "expanded": true,
                "pages": [
                  "products/yield/yield_intro",
                  {
                    "group": "Tron Overnight Yield",
                    "tag": "6.0% APY",
                    "expanded": true,
                    "pages": [
                      "products/yield/overview",
                      "products/yield/architecture",
                      "products/yield/user-flow",
                      "products/yield/faq",
                      "products/yield/risks"
                    ]
                  }
                ]
              },
              {
                "group": "Cross-chain Swap",
                "icon": "bridge",
                "tag": "0% FEE",
                "expanded": true,
                "pages": [
                  "products/bridge/overview",
                  "products/bridge/faq"
                ]
              }
            ]
          },
          {
            "group": "Resources",
            "pages": [
              "resources/contracts",
              "resources/audits",
              "resources/brand-assets",
              "resources/support"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/l2pass_docs_light.svg",
    "dark": "/logo/l2pass_docs_dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "/resources/support"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Start Earning",
      "href": "https://earn.l2pass.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/l2_pass",
      "github": "https://github.com/l2pass",
      "telegram": "https://t.me/l2pass"
    }
  }
}